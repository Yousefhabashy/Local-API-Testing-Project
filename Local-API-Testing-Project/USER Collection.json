      "name": "Create-User",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "var requestData = pm.request.body.raw;\r",
              "let jsonData = JSON.parse(requestData);\r",
              "\r",
              "pm.environment.set(\"email\", jsonData.email);\r",
              "pm.environment.set(\"password\", jsonData.password);\r",
              "\r",
              "pm.test(\"Account cerated\", function () {\r",
              "    pm.response.to.have.status(201);\r",
              "});\r",
              "\r",
              "pm.test(\"Response time is less than 200ms\", function () {\r",
              "    pm.expect(pm.response.responseTime).to.be.below(200);\r",
              "});\r",
              "\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "raw",
          "raw": "{\r\n  \"email\": \"yousefhabashy10233@gmail.com\",\r\n  \"password\": \"Yousef@123\",\r\n  \"name\": \"Yousef Habashy\",\r\n  \"role\": \"admin\"\r\n}",
